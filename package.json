{
  "name": "galactic-storage",
  "version": "0.1.0",
  "description": "Hook for working with React Native Async Storage",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "jest --watch",
    "clean": "rm -rf dist && mkdir dist",
    "build": "npm run clean && npm run build:types && npm run build:js",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel src --out-dir dist --extensions \".ts,.txt\" --source-maps inline",
    "prepare": "npm run build",
    "ts": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/baron816/Galactic-Storage.git"
  },
  "keywords": [
    "react",
    "react-native",
    "async-storage",
    "hooks"
  ],
  "author": "Baron Willeford",
  "license": "MIT",
  "peerDependencies": {
    "react": "^16.8.0 || 17.0.0",
    "@react-native-async-storage/async-storage": "^1.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-typescript": "^7.12.7",
    "@types/jest": "^26.0.19",
    "@types/react": "^17.0.0",
    "babel-jest": "^26.6.3",
    "jest": "^26.6.3",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.13.2",
    "react": "^17.0.1"
  }
}
